      "mentionedUsers": [
        1036286798295691264
      ],
      "media": []
    },
    {
      "disorder_flag": false,
      "text": "  It\u2019s not a personality disorder. That\u2019s OCPD.",
      "conversation_id": 1171508919539122179,
      "tweet_id": 1171546510821138434,
      "language": "en",
      "likes_count": 0,
      "quote_count": 0,
      "reply_count": 1,
